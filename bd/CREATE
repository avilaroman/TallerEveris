CREATE TABLE arsv_solicitudes (
    soli_id bigint NOT NULL,
    soli_mtow bigint,
    soli_detalle_antecedentes_solicitados character varying(5000),
    soli_callsign character varying(50),
    soli_cantidad_vuelos bigint,
    soli_consignatario character varying(50),
    soli_mail character varying(50),
    soli_mail_operador character varying(50) NOT NULL,
    soli_detalle_solicitud character varying(5000),
    soli_direccion character varying(50),
    soli_dfacturacion character varying(50) NOT NULL,
    soli_embarcador character varying(50),
    soli_fecha_ingreso timestamp without time zone,
    soli_fecha_cierre timestamp without time zone,
    soli_fecha_creacion timestamp without time zone,
    soli_fecha_salida timestamp without time zone,
    soli_nombre_empresa character varying(50) NOT NULL,
    soli_piloto character varying(50),
    soli_observaciones character varying(5000),
    soli_razon_social character varying(50),
    sac_cmatricula character varying(50) NOT NULL,
    soli_vigencia_seguro timestamp without time zone,
    esso_codigo_estados_solicitudes character varying(255),
    oaci_codigo_oaci character varying(255),
    clas_codigo_clasificacion character varying(255),
    obvu_codigo_objeto character varying(255),
    paor_id_pais_aeronave bigint,
    paor_id_pais bigint,
    tiso_codigo_tipo_solicitud character varying(255),
    unpe_codigo_unidad character varying(255),
    CONSTRAINT arsv_solicitudes_soli_cantidad_vuelos_check CHECK ((soli_cantidad_vuelos >= 0)),
    CONSTRAINT arsv_solicitudes_soli_mtow_check CHECK ((soli_mtow >= 0))
);


ALTER TABLE public.arsv_solicitudes OWNER TO arsv;

